doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    title IQAC Form
    link(rel='stylesheet', href='../css/normalize.min.css')
    link(rel='stylesheet', href='../css/form.css')
  body
    .top-cont
      .top-line
        div(style='float: right')
          button#back(type='button') BACK
        div(style='float: left')
          button(type='submit') SAVE
    .bot-cont
      form(action='index.html', method='post')
        h1 Criterion – II
        h2 CSE
        fieldset
          legend
            span.number 2
            | Teaching, Learning and Evaluation
          // 2.6 & 2.12 TO BE FILLED BY HOQ
          label 2.1 Total No. of permanent faculty:
          table(style='width:100%')
            tr
              th Asst. Professors
              th Associate Professors
              th Professors
              th Others
              th Total
            tr
              td
                input(type='number', name='r1c2', value='')
              td
                input(type='number', name='r1c3', value='')
              td
                input(type='number', name='r1c4', value='')
              td
                input(type='number', name='r1c5', value='')
              td
                input(type='number', name='r1c6', value='')
          label 2.2 No. of permanent faculty with Ph.D.:
          input(type='number', name='2.2')
          label 2.3 No. of Faculty Positions Recruited (R) and Vacant (V) during the year
          table(style='width:100%')
            tr
              th(colspan='2') Asst. Professors
              th(colspan='2') Associate Professors
              th(colspan='2') Professors
              th(colspan='2') Others
              th(colspan='2') Total
            tr
              th R
              th V
              th R
              th V
              th R
              th V
              th R
              th V
              th R
              th V
            tr
              td
                input(type='number', name='r3c1', value='')
              td
                input(type='number', name='r3c2', value='')
              td
                input(type='number', name='r3c3', value='')
              td
                input(type='number', name='r3c4', value='')
              td
                input(type='number', name='r3c5', value='')
              td
                input(type='number', name='r3c6', value='')
              td
                input(type='number', name='r3c7', value='')
              td
                input(type='number', name='r3c8', value='')
              td
                input(type='number', name='r3c9', value='')
              td
                input(type='number', name='r3c10', value='')
          label 2.4 No. of Guest and Visiting faculty and Temporary faculty:
          table(style='width:100%')
            tr
              th Guest
              th Visiting Faculty
              th Temporary
            tr
              td
                input(type='number', name='r4c1', value='')
              td
                input(type='number', name='r4c2', value='')
              td
                input(type='number', name='r4c3', value='')
          label 2.5 Faculty participation in conferences and symposia:
          table(style='width:100%')
            tr
              th(colspan='3') International Level
              th(colspan='3') National Level
              th(colspan='3') State Level
            tr
              th Attended Seminars/ Workshops
              th Presented papers
              th Resource Persons
              th Attended Seminars/ Workshops
              th Presented papers
              th Resource Persons
              th Attended Seminars/ Workshops
              th Presented papers
              th Resource Persons
            tr
